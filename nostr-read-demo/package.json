{
  "name": "nostr-read-demo",
  "version": "1.0.0",
  "source": "src/main.js",
  "main": "out/main.js",
  "license": "MIT",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "parcel": "^2.12.0",
    "typescript": "^5.4.3"
  },
  "scripts": {
    "build": "parcel build --no-optimize --no-scope-hoist",
    "run": "node --enable-source-maps out/main.js",
    "dev": "yarn build && yarn run run"
  },
  "dependencies": {
    "@kvs/env": "^2.2.0",
    "@kvs/node-localstorage": "^2.2.0",
    "borsher": "^1.2.1",
    "dephy-borsh-types": "^0.0.2",
    "nostr-tools": "^2.3.2"
  },
  "engines": {
    "node": ">= 20"
  },
  "targets": {
    "main": {
      "includeNodeModules": true,
      "context": "node",
      "scopeHoist": true,
      "optimize": false
    }
  }
}
